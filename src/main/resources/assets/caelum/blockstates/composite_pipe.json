{
  "multipart": [
    {
      "apply": { "model": "caelum:block/composite_pipe_core" }
    },

    { "when": { "north": "pipe" },   "apply": { "model": "caelum:block/composite_pipe_side" } },
    { "when": { "north": "input" },  "apply": { "model": "caelum:block/composite_pipe_input" } },
    { "when": { "north": "output" }, "apply": { "model": "caelum:block/composite_pipe_output" } },

    { "when": { "east": "pipe" },    "apply": { "model": "caelum:block/composite_pipe_side", "y": 90 } },
    { "when": { "east": "input" },   "apply": { "model": "caelum:block/composite_pipe_input", "y": 90 } },
    { "when": { "east": "output" },  "apply": { "model": "caelum:block/composite_pipe_output", "y": 90 } },

    { "when": { "south": "pipe" },   "apply": { "model": "caelum:block/composite_pipe_side", "y": 180 } },
    { "when": { "south": "input" },  "apply": { "model": "caelum:block/composite_pipe_input", "y": 180 } },
    { "when": { "south": "output" }, "apply": { "model": "caelum:block/composite_pipe_output", "y": 180 } },

    { "when": { "west": "pipe" },    "apply": { "model": "caelum:block/composite_pipe_side", "y": 270 } },
    { "when": { "west": "input" },   "apply": { "model": "caelum:block/composite_pipe_input", "y": 270 } },
    { "when": { "west": "output" },  "apply": { "model": "caelum:block/composite_pipe_output", "y": 270 } },

    { "when": { "up": "pipe" },      "apply": { "model": "caelum:block/composite_pipe_side", "x": 270 } },
    { "when": { "up": "input" },     "apply": { "model": "caelum:block/composite_pipe_input", "x": 270 } },
    { "when": { "up": "output" },    "apply": { "model": "caelum:block/composite_pipe_output", "x": 270 } },

    { "when": { "down": "pipe" },    "apply": { "model": "caelum:block/composite_pipe_side", "x": 90 } },
    { "when": { "down": "input" },   "apply": { "model": "caelum:block/composite_pipe_input", "x": 90 } },
    { "when": { "down": "output" },  "apply": { "model": "caelum:block/composite_pipe_output", "x": 90 } }
  ]
}